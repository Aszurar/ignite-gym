{
  "name": "ignite-gym-back",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "lint": "eslint src --ext .ts --fix",
    "build": "tsup src --out-dir build",
    "start": "node build/server.js",
    "dev:env": "dotenv -e .env.local -- tsx watch src/server.ts",
    "migrate-dev:postgres": "dotenv -e .env.local -- npx prisma migrate dev",
    "migrate-prod:postgres": "dotenv -e .env.local -- npx prisma migrate deploy",
    "prisma-studio": "dotenv -e .env.local -- npx prisma studio",
    "test": "dotenv -e .env.local -- jest -i"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "6.5.0",
    "bcryptjs": "3.0.2",
    "dotenv": "16.4.7",
    "dotenv-cli": "8.0.0",
    "fastify": "5.2.1",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "9.22.0",
    "@types/node": "22.13.10",
    "@typescript-eslint/eslint-plugin": "8.26.1",
    "@typescript-eslint/parser": "8.26.1",
    "eslint": "9.22.0",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "globals": "16.0.0",
    "prettier": "3.5.3",
    "prisma": "6.5.0",
    "tsup": "8.4.0",
    "tsx": "4.19.3",
    "typescript": "5.8.2",
    "typescript-eslint": "8.26.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
